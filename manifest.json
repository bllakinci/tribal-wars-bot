{
  "name": "Tribal Wars Multi Attack",
  "description": "Allows you to easily fake attack with 100ms interval.",
  "version": "2.3",
  "manifest_version": 2,
  "browser_action": {
    "default_popup": "default_popup/popup.html"
  },
  "permissions": [
    "*://*.die-staemme.de/",
    "*://*.staemme.ch/",
    "*://*.tribalwars.net/",
    "*://*.tribalwars.nl/",
    "*://*.plemiona.pl/",
    "*://*.tribalwars.se/",
    "*://*.tribalwars.com.br/",
    "*://*.tribalwars.com.pt/",
    "*://*.divokekmeny.cz/",
    "*://*.triburile.ro/",
    "*://*.voyna-plemyon.ru/",
    "*://*.fyletikesmaxes.gr/",
    "*://*.tribalwars.com/",
    "*://*.divoke-kmene.sk/",
    "*://*.klanhaboru.hu/",
    "*://*.tribalwars.dk/",
    "*://*.tribals.it/",
    "*://*.klanlar.org/",
    "*://*.guerretribale.fr/",
    "*://*.guerrastribales.es/",
    "*://*.tribalwars.ae/",
    "*://*.tribalwars.co.uk/",
    "*://*.vojnaplemen.si/",
    "*://*.plemena.com/",
    "*://*.tribalwars.asia/",
    "*://*.tribalwars.works/",
    "*://*.tribalwars.us/"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "*://*.die-staemme.de/*",
        "*://*.staemme.ch/*",
        "*://*.tribalwars.net/*",
        "*://*.tribalwars.nl/*",
        "*://*.plemiona.pl/*",
        "*://*.tribalwars.se/*",
        "*://*.tribalwars.com.br/*",
        "*://*.tribalwars.com.pt/*",
        "*://*.divokekmeny.cz/*",
        "*://*.triburile.ro/*",
        "*://*.voyna-plemyon.ru/*",
        "*://*.fyletikesmaxes.gr/*",
        "*://*.tribalwars.com/*",
        "*://*.divoke-kmene.sk/*",
        "*://*.klanhaboru.hu/*",
        "*://*.tribalwars.dk/*",
        "*://*.tribals.it/*",
        "*://*.klanlar.org/*",
        "*://*.guerretribale.fr/*",
        "*://*.guerrastribales.es/*",
        "*://*.tribalwars.ae/*",
        "*://*.tribalwars.co.uk/*",
        "*://*.vojnaplemen.si/*",
        "*://*.plemena.com/*",
        "*://*.tribalwars.asia/*",
        "*://*.tribalwars.works/*",
        "*://*.tribalwars.us/*"
      ],
      "js": [
        "inject.js"
      ],
      "all_frames": true
    }
  ],
  "web_accessible_resources": [
    "lib/*.js",
    "css/*.css"
  ],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}